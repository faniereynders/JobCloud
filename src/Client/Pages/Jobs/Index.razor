@page "/jobs"

<PageTitle>Jobs</PageTitle>


    <MudToolBar Class="mb-6" DisableGutters="true" Dense="true">
        <MudButton Class="mr-1" StartIcon="@Icons.Material.Filled.Add" Size="Size.Large" Icon="@Icons.Material.Outlined.Add" Variant="Variant.Filled" Color="Color.Primary">New job</MudButton>
        <MudIconButton  Icon="@Icons.Material.Outlined.Refresh" />
        
    </MudToolBar>

<MudTabs  Rounded="true" ApplyEffectsToContainer="true" >
    <MudTabPanel Text="Pending">
        <Jobs/>
    </MudTabPanel>
    <MudTabPanel Text="Completed">
        <Jobs/>
    </MudTabPanel>
    
</MudTabs>